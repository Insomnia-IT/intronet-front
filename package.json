{
  "name": "@insomnia/intro-front",
  "version": "1.0.0",
  "description": "Intronet web site for insomnia fest",
  "scripts": {
    "compile": "cmmn compile --watch",
    "bundle": "cmmn bundle",
    "run": "cmmn bundle --watch --run",
    "start": "react-scripts start",
    "story": "start-storybook -p 6006",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "ci": "react-scripts build --prod && cd build && docker build . -t insomnia/intronet-front",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.0",
    "@chakra-ui/react": "^2.0.2",
    "@cmmn/cell": "link://~/dev/cmmn/cell",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@types/luxon": "^2.3.1",
    "@types/node": "17.x.x",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "chakra-react-select": "^4.0.0",
    "chakra-ui-markdown-renderer": "^4.1.0",
    "cookie": "^0.5.0",
    "dexie": "3.x.x",
    "formik": "^2.2.9",
    "framer-motion": "^6",
    "luxon": "2.x.x",
    "pdfjs": "2.x.x",
    "qr-scanner": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.3",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.x.x",
    "sass": "1.50.0",
    "typescript": "4.6.4",
    "use-cookie-state": "^2.0.0"
  },
  "devDependencies": {
    "@cmmn/tools": "link://~/dev/cmmn/tools",
    "@storybook/addon-actions": "6.5.0-rc.0",
    "@storybook/addon-essentials": "6.5.0-rc.0",
    "@storybook/addon-links": "6.5.0-rc.0",
    "@storybook/builder-webpack5": "6.5.0-rc.0",
    "@storybook/manager-webpack5": "6.5.0-rc.0",
    "@storybook/preset-create-react-app": "4.x.x",
    "@storybook/react": "6.5.0-rc.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.2",
    "typescript-transform-paths": "3.3.1"
  },
  "author": "",
  "license": "ISC",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "https://intro.cherepusick.keenetic.name",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ]
  },
  "cmmn": {
    "main": {
      "input": "dist/esm/src/index.js",
      "module": "es",
      "port": 3001,
      "styles": "modules",
      "html": "assets/index.html"
    }
  }
}
